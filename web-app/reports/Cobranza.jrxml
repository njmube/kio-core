	<queryString><![CDATA[SELECT  c.nombre,v.id,ifnull(concat(d.folio,' ',d.serie),'') as folio_cfdi,v.total as total_fac,a.importe as cobrado,p.forma_de_pago
,a.fecha,v.fecha as fecha_fac
,(case when p.forma_de_pago ='EFECTIVO' THEN a.importe ELSE 0 END) AS 'EFECTIVO'
,(case when p.forma_de_pago ='CHEQUE' THEN a.importe ELSE 0 END) AS 'CHEQUE'
,(case when p.forma_de_pago ='TARJETA_DE_CREDITO' THEN a.importe ELSE 0 END) AS 'TARJETA_DE_CREDITO'
,(case when p.forma_de_pago ='TARJETA_DE_DEBITO' THEN a.importe ELSE 0 END) AS 'TARJETA_DE_DEBITO'
FROM aplicacion_de_pago a join pago p on (p.id=a.pago_id) join venta v on (v.id=a.venta_id) join cliente c on (c.id=v.cliente_id) left join cfdi d on (d.origen=v.id)
where a.fecha=$P{FECHA}]]></queryString>

